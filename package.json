{
  "name": "deleight",
  "version": "3.0.0",
  "description": "A group of 10 libraries for writing accessible and joyfully interactive web applications with traditional HTML, CSS and JavaScript.",
  "type": "module",
  "exports": {
    "./class-action": {
      "require": "./dist/class-action/cjs/class-action.js",
      "default": "./dist/class-action/esm/class-action.js"
    },
    "./action-object": {
      "require": "./dist/action-object/cjs/action-object.js",
      "default": "./dist/action-object/esm/action-object.js"
    },
    "./active-component": {
      "require": "./dist/active-component/cjs/active-component.js",
      "default": "./dist/active-component/esm/active-component.js"
    },
    "./reftype": {
      "require": "./dist/reftype/cjs/reftype.js",
      "default": "./dist/reftype/esm/reftype.js"
    },
    "./appliance": {
      "require": "./dist/appliance/cjs/appliance.js",
      "default": "./dist/appliance/esm/appliance.js"
    },
    "./actribute": {
      "require": "./dist/actribute/cjs/actribute.js",
      "default": "./dist/actribute/esm/actribute.js"
    },
    "./eutility": {
      "require": "./dist/eutility/cjs/eutility.js",
      "default": "./dist/eutility/esm/eutility.js"
    },
    "./generational": {
      "require": "./dist/generational/cjs/generational.js",
      "default": "./dist/generational/esm/generational.js"
    },
    "./apriori": {
      "require": "./dist/apriory/cjs/apriori.js",
      "default": "./dist/apriori/esm/apriori.js"
    },
    "./sophistry": {
      "require": "./dist/sophistry/cjs/sophistry.js",
      "default": "./dist/sophistry/esm/sophistry.js"
    },
    "./queryoperator": {
      "require": "./dist/queryoperator/cjs/queryoperator.js",
      "default": "./dist/queryoperator/esm/queryoperator.js"
    },
    "./onetomany": {
      "require": "./dist/onetomany/cjs/onetomany.js",
      "default": "./dist/onetomany/esm/onetomany.js"
    },
    "./withly": {
      "require": "./dist/withly/cjs/withly.js",
      "default": "./dist/withly/esm/withly.js"
    }
  },
  "scripts": {
    "test": "node --experimental-modules --test",
    "otest": "node --test-only --experimental-modules --test",
    "wtest": "node --experimental-modules --test --watch",
    "doc": "npx typedoc --out ./docs//api/deleight-api-docs/main ./src/*.ts",
    "compile": "npx tsc",
    "build": "npx tsc && rollup -c",
    "dist": "rollup -c",
    "start": "node server.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mksunny1/deleight"
  },
  "homepage": "https://github.com/mksunny1/deleight",
  "keywords": [
    "HTML",
    "CSS",
    "JavaScript",
    "vanilla",
    "reactivity",
    "accessibility",
    "tradition",
    "Deleight",
    "Actribute",
    "Appliance",
    "Reftype",
    "QueryOperator",
    "Sophistry",
    "Apriori",
    "OneToMany",
    "Generational",
    "Eutility",
    "withly"
  ],
  "author": "Mark Sun",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "express": "^4.19.2",
    "jsdom": "^24.0.0",
    "rollup": "^4.14.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "action-object": "^1.0.2",
    "active-component": "^1.0.0",
    "class-action": "^1.0.2"
  }
}
