{
  "name": "deleight",
  "version": "1.2.0",
  "description": "A group of 8 libraries for writing accessible and joyfully interactive web applications with traditional HTML, CSS and JavaScript.",
  "type": "module",
  "exports": {
    "./appliance": {
      "require": "./dist/appliance/cjs/appliance.js",
      "default": "./dist/appliance/esm/appliance.js"
    },
    "./actribute": {
      "require": "./dist/actribute/cjs/actribute.js",
      "default": "./dist/actribute/esm/actribute.js"
    },
    "./eventivity": {
      "require": "./dist/eventivity/cjs/eventivity.js",
      "default": "./dist/eventivity/esm/eventivity.js"
    },
    "./generational": {
      "require": "./dist/generational/cjs/generational.js",
      "default": "./dist/generational/esm/generational.js"
    },
    "./apriori": {
      "require": "./dist/apriory/cjs/apriori.js",
      "default": "./dist/apriori/esm/apriori.js"
    },
    "./sophistry": {
      "require": "./dist/sophistry/cjs/sophistry.js",
      "default": "./dist/sophistry/esm/sophistry.js"
    },
    "./domitory": {
      "require": "./dist/domitory/cjs/domitory.js",
      "default": "./dist/domitory/esm/domitory.js"
    },
    "./onetomany": {
      "require": "./dist/onetomany/cjs/onetomany.js",
      "default": "./dist/onetomany/esm/onetomany.js"
    },
    "./withy": {
      "require": "./dist/withy/cjs/withy.js",
      "default": "./dist/withy/esm/withy.js"
    }
  },
  "scripts": {
    "test": "node --experimental-modules --test",
    "otest": "node --test-only --experimental-modules --test",
    "wtest": "node --experimental-modules --test --watch",
    "doc": "npx typedoc --out ./docs//api/deleight-api-docs/main ./src/*.ts",
    "compile": "npx tsc",
    "build": "npx tsc && rollup -c",
    "dist": "rollup -c",
    "start": "node server.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mksunny1/deleight"
  },
  "homepage": "https://github.com/mksunny1/deleight",
  "keywords": [
    "HTML",
    "CSS",
    "JavaScript",
    "vanilla",
    "reactivity",
    "accessibility",
    "tradition",
    "Deleight",
    "Apriori",
    "Eventivity",
    "Sophistry",
    "OneToMany",
    "Appliance",
    "Domitory",
    "Actribute",
    "Generational",
    "withy"
  ],
  "author": "Mark Sun",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "express": "^4.19.2",
    "jsdom": "^24.0.0",
    "rollup": "^4.14.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
